<resources>
    <!-- Base application theme. -->
    <style name="Theme.Gdelete" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#E91E63</item>
        <item name="colorPrimaryVariant">#FF0057</item>
        <item name="colorOnPrimary">#FF0057</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FF0057</item>
        <item name="colorSecondaryVariant">#FF0057</item>
        <item name="colorOnSecondary">#FF0057</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#FF0057</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="CustomSplashTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- below one -->
        <item name="android:windowBackground">@drawable/spl</item>
    </style>
</resources>